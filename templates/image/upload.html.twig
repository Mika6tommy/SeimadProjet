{# templates/image/upload.html.twig #}

{% extends 'layouts/base.html.twig' %}

{% block title %}Upload Image{% endblock %}

{% block body %}
    <h1>Upload Image</h1>

    {{ form_start(form) }}
    {{ form_row(form.name) }}
    {{ form_row(form.ImageData) }}
    <button type="submit">Upload</button>
    {{ form_end(form) }}
{% endblock %}
